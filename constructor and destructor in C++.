#include <iostream>
using namespace std;
class Demo {
public:
    Demo() {  
        cout << "Default Constructor Called!" << endl;
    }
      ~Demo() { // Destructor
        cout << "Destructor Called!" << endl;
    }
};
int main() {
    Demo obj; 
    cout << "Inside main function" << endl;
    return 0; 
}

