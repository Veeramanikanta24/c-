#include <iostream>
#include <map>
using namespace std;
int main() {
    map<int, string> m;
    
    m[1] = "One";
    m[2] = "Two";
    m[3] = "Three";
    // Traversal
    cout << "Map Elements:" << endl;
    for (auto &p : m)
        cout << p.first << " -> " << p.second << endl;
    
    if (m.find(2) != m.end())
        cout << "Found Key 2: " << m[2] << endl;
  
    m.erase(1);
    cout << "After Deletion:" << endl;
    for (auto &p : m)
        cout << p.first << " -> " << p.second << endl;
    return 0;
}
